<Window x:Class="改元ユニコーン.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:改元ユニコーン"
        mc:Ignorable="d"
        xmlns:System="clr-namespace:System;assembly=mscorlib"
        Title="改元ユニコーン" SizeToContent="WidthAndHeight" ResizeMode="NoResize" ContentRendered="Window_ContentRendered">
    <StackPanel Margin="5">
        <TextBlock Margin="3" Text="改元の日付を指定"/>
        <DatePicker x:Name="detePicker1" Margin="3" SelectedDateFormat="Long" SelectedDate="2019/5/1" Width="150"/>
        <Button Margin="3" Content="終了" Click="Button_Click"/>
    </StackPanel>
</Window>
